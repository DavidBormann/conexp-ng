<?xml version="1.0" encoding="UTF-8"?>
<lattice>
    <name>Hier steht der Name des Verbands</name>
    <date>Das Datum an dem es gespeichert wurde</date>
    <author>Der Name des Autors</author>
    <!--Liste von Knoten-->
    <list-of-nodes>
        <!-- Jeder Punkt hat eine eindeutige ID, die absoluten (x,y)-Koordinaten, eine Menge von Attributen und eine Menge von Objekten -->
        <node>
            <id>Hier steht die ID des Knoten</id>
            <x>absolute x-Koordinate</x>
            <y>absolute y-Koordinate</y>
            <set-of-objects>
                <!-- Jedes Objekt hat einen Namen -->
                <obeject>
                    <object-name>Name des Objektes</object-name>
                </object>
            </set-of-objects>
            <set-of-attributes>
                <!-- Jedes Attribut hat einen Namen -->
                <attribute>
                    <attribute-name>Name des Attributes</attribute-name>
                </attribute>
            </set-of-attributes>
        </node>
    </list-of-nodes>
    <!-- Liste von Kanten -->
    <list-of-edges>
        <!-- Eine Kante besteht genau aus zwei Knoten. Der Verweis auf die Knoten geschieht Ã¼ber die ID. -->
        <edge>
            <start>Startknoten.ID</start>
            <dest>Zielknoten.ID</dest>
        </edge>
    </list-of-edges>
    <!-- Menge von Assoziationen -->
    <set-of-associations>
        <!-- Eine Assoziation X -> Y hat eine Linkeseite X, eine Rechtseite Y, eine Supportwert, sowie einen Confidencewert. -->
        <association>
            <x>Linkeseite der Assoziation</x>
            <y>Rechtseite der Assoziation</y>
            <support>Supportwert</support>
            <!-- An dieser Stelle kann man sagen, dass wenn c=100% ist, dann ist es eine Implikation. -->
            <confidence>Confidencewert</confidence>
        </association>
    </set-of-associations>
</lattice>
