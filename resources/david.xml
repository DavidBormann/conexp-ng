<?xml version="1.0" encoding="UTF-8"?>
<concepts>
  <date>Das Datum an dem es gespeichert wurde</date>

  <!-- Aufbau wie die Reihenfolge der Views -->

  <attributes>
    <!-- Liste der Attribute -->
  </attributes>

  <objects>
    <!-- Liste der Objekte mit Verweis auf die vorkommenden Atribute -->
  </objects>

  <!-- Lattice von Jan, die Attribute und Objekte eines Knoten müsste dan
    angepasst werden -->
  <lattice>
    <!--Liste von Knoten -->
    <list-of-nodes>
      <!-- Jeder Punkt hat eine eindeutige ID, die absoluten (x,y)-Koordinaten,
        eine Menge von Attributen und eine Menge von Objekten -->
      <node>
        <id>Hier steht die ID des Knoten</id>
        <x>absolute x-Koordinate</x>
        <y>absolute y-Koordinate</y>
        <set-of-objects>
          <!-- Jedes Objekt hat einen Namen -->
          <obeject>
            <object-name>Name des Objektes</object-name>
          </object>
        </set-of-objects>
        <set-of-attributes>
          <!-- Jedes Attribut hat einen Namen -->
          <attribute>
            <attribute-name>Name des Attributes</attribute-name>
          </attribute>
        </set-of-attributes>
      </node>
    </list-of-nodes>
    <!-- Liste von Kanten -->
    <list-of-edges>
      <!-- Eine Kante besteht genau aus zwei Knoten. Der Verweis auf die Knoten
        geschieht über die ID. -->
      <edge>
        <start>Startknoten.ID</start>
        <dest>Zielknoten.ID</dest>
      </edge>
    </list-of-edges>
  </lattice>

  <!-- Menge von Assoziationen -->
  <set-of-associations>
    <!-- Eine Assoziation X -> Y hat eine Linkeseite X, eine Rechtseite Y,
      eine Supportwert, sowie einen Confidencewert. -->
    <association>
      <antecedent>Linke Seite der Assoziation als Liste von Attributen
      </antecedent>
      <consequence>Rechte Seite der Assoziation als Liste von Attributen
      </consequence>
      <support>Supportwert</support>
      <!-- An dieser Stelle kann man sagen, dass wenn c=100% ist, dann ist es
        eine Implikation. -->
      <confidence>Confidencewert</confidence>
    </association>
  </set-of-associations>
</concepts>
